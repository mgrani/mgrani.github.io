  bdsk-url-1	= {http://ercim-news.ercim.eu/en96/special/lost-in-semantics-ballooning-the-web-of-data}
}

@InProceedings{	  14hoe.gra.vea.ea-Linked-Data-Query-Wizard,
  abstract	= {In an interconnected world, Linked Data is more important
		  than ever before. However, it is still quite difficult to
		  access this new wealth of semantic data directly without
		  having in-depth knowledge about {SPARQL} and related
		  semantic technologies. Also, most people are currently used
		  to con- suming data as 2-dimensional tables. Linked Data is
		  by defi- nition always a graph, and not that many people
		  are used to handle data in graph structures. Therefore we
		  present the Linked Data Query Wizard, a web-based tool for
		  displaying, accessing, filtering, exploring, and navigating
		  Linked Data stored in {SPARQL} endpoints. The main
		  innovation of the interface is that it turns the graph
		  structure of Linked Data into a tabular interface and
		  provides easy-to-use interaction possibilities by using
		  metaphors and techniques from current search engines and
		  spreadsheet applications that regular web users are already
		  familiar with.},
  author	= {Hoefler, Patrick and Granitzer, Michael and Veas, Eduardo
		  and Seifert, Christin},
  booktitle	= {Proceedings of Linked Data on the Web ({LDOW}) at {WWW}
		  2014},
  file		= {media/papers/14hoe.gra.vea.ea-Linked-Data-Query-Wizard.pdf},
  keywords	= {Linked Data, {RDF} Data Cube, {SPARQL}, User Interface},
  location	= {Seoul},
  title		= {Linked Data Query Wizard: A Novel Interface for Accessing
		  {SPARQL} Endpoints},
  url		= {http://events.linkeddata.org/ldow2014/papers/ldow2014_paper_06.pdf},
  year		= 2014,
  bdsk-url-1	= {http://events.linkeddata.org/ldow2014/papers/ldow2014_paper_06.pdf}
}

@Article{	  14kla.gra.jac.ea-Unsupervised-document-structure-analysis,
  abstract	= {Text mining and information retrieval in large collections
		  of scientific literature require automated processing
		  systems that analyse the documents' content. However, the
		  layout of scientific articles is highly varying across
		  publishers, and common digital document formats are
		  optimised for presentation, but lack structural
		  information. To overcome these challenges, we have
		  developed a processing pipeline that analyses the structure
		  a {PDF} document using a number of unsupervised machine
		  learning techniques and heuristics. Apart from the
		  meta-data extraction, which we reused from previous work,
		  our system uses only information available from the current
		  document and does not require any pre-trained model. First,
		  contiguous text blocks are extracted from the raw character
		  stream. Next, we determine geometrical relations between
		  these blocks, which, together with geometrical and font
		  information, are then used categorize the blocks into
		  different classes. Based on this resulting logical
		  structure we finally extract the body text and the table of
		  contents of a scientific article. We separately evaluate
		  the individual stages of our pipeline on a number of
		  different datasets and compare it with other document
		  structure analysis approaches. We show that it outperforms
		  a state-of-the-art system in terms of the quality of the
		  extracted body text and table of contents. Our unsupervised
		  approach could provide a basis for advanced digital library
		  scenarios that involve diverse and dynamic corpora.},
  author	= {Klampfl, Stefan and Granitzer, Michael and Jack, Kris and Kern, Roman},
  file		= {media/papers/14kla.gra.jac.ea-Unsupervised-document-structure-analysis.pdf},
  title		= {Unsupervised document structure analysis of digital scientific articles},
  journal={International Journal on Digital Libraries},
  volume={14},
  number={3-4},
  pages={83--99},
  year={2014},
  publisher={Springer},
}

