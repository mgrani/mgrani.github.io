@InProceedings{	  14gra.ste.hoe.ea-Linked-Data-in-Context,
  abstract	= {Linked Open Data has grown into a large and recognized
		  source of data, however its uptake and commercial
		  exploitation does not yet reflect its potential value. Two
		  factors with potential to contribute to the value of data
		  are correlating previously uncorrelated data and providing
		  answers based on the data. We present a data-centric
		  question answering portal focusing on answers based on
		  empirical facts embedded in numerical Linked Open Data. The
		  portal is backed by a number of services addressing i) data
		  extraction and semantic integration, ii) creating, merging
		  and extending data sets, and iii) discovery and visual
		  analysis targeting {IT} laymen.},
  author	= {Granitzer, Michael and Stegmaier, Florian and Hoefler,
		  Patrick and Kern, Roman and Sabol, Vedran},
  booktitle	= {European Data Forum 2014},
  title		= {Linked Data in Context: Questions Matter},
